  "-DCMAKE_CXX_FLAGS=-std=c++17 -O3 -funroll-loops -fPIC" \
  -DCMAKE_LIBRARY_OUTPUT_DIRECTORY=/home/engine/project/app/build/intermediates/cxx/RelWithDebInfo/4o694b1q/obj/arm64-v8a \
